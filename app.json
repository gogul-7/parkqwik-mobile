{
  "expo": {
    "name": "parkqwikmobile",
    "slug": "parkqwikmobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/headerback.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/headerback.png",
        "backgroundColor": "white"
      }
    },
    "web": {
      "favicon": "./app/assets/images/headerback.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow ParkQwik to access camera."
        }
      ]
    ]
  }
}
